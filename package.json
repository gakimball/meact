  },
  "homepage": "https://github.com/gakimball/meact#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "cheerio": "^0.22.0",
    "enzyme": "^2.6.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "react": "^15.4.1",
